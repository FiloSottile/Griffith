<!-- ITEMS -->
	<tr<@id> id="i@DATA@"</@id>><@image>
		<td class="image"><img src="./posters/@DATA@.jpg" alt="poster" /></td></@image><@number>
		<td class="number">@DATA@</td></@number><@title>
		<td class="title">@DATA@</td></@title><@original_title>
		<td class="otitle">@DATA@</td></@original_title><@year>
		<td class="year">@DATA@</td></@year><@director>
		<td class="director">@DATA@</td></@director><@rating>
		<td class="rating">@DATA@</td></@rating><@runtime>
		<td class="runtime">@DATA@</td></@runtime><@country>
		<td class="country">@DATA@</td></@country><@genre>
		<td class="genre">@DATA@</td></@genre><@imdb>
		<td class="links"><a href="@DATA@">link</a></td></@imdb><@site>
		<td class="links"><a href="@DATA@">link</a></td></@site><@trailer>
		<td class="links"><a href="@DATA@">link</a></td></@trailer><@media>
