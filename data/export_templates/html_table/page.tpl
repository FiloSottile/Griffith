<body>
<@header><h1>@DATA@</h1></@header>

<table>
	<thead>
		<tr><@image>
			<th>@TITLE@</th></@image><@number>
			<th>@TITLE@</th></@number><@title>
			<th>@TITLE@</th></@title><@original_title>
